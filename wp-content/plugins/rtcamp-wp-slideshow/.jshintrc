{
    "globals": {
        "jQuery": false,
        "$": false,
        "Swiper": false
    },
    "strict": "global",
    "maxparams": 3,
    "maxdepth": 3,
    "maxstatements": 15,
    "maxcomplexity": 5,
    "maxlen": 100,
    "asi": false,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "undef": true,
    "unused": "vars"
}